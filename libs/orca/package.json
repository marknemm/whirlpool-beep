{
  "name": "@npc/orca",
  "version": "0.0.1",
  "description": "Orca Beep-Boop-Bop ğŸ¤–",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc --build",
    "build:watch": "tsc --build --watch",
    "build:fn": "npm run prod:package",
    "clean": "rimraf dist .serverless wallet.json",
    "clean:all": "npm run clean && npm run clean:deps && npm run clean:logs",
    "clean:deps": "rimraf node_modules",
    "clean:logs": "rimraf logs",
    "dev": "npm run build && npm run dev:env -- npm run start:ts --",
    "dev:db": "docker-compose up -d db",
    "dev:deploy": "serverless deploy --stage development",
    "dev:env": "npm run dev:db && dotenv -e .env.development.local -e ../solana/.env.development.local -e ../core/.env.development.local -e .env.development -e ../solana/.env.development -e ../core/.env.development -e .env -e ../solana/.env -e ../core/.env --",
    "dev:package": "serverless package --stage development",
    "dev:sls": "npm run dev:env -- serverless invoke local --stage development --function",
    "install:all": "npm install --include-workspace-root",
    "lint": "eslint .",
    "prod": "npm run build && npm run prod:env -- npm run start:ts --",
    "prod:deploy": "serverless deploy --stage production",
    "prod:env": "dotenv -e .env.production.local -e ../solana/.env.production.local -e ../core/.env.production.local -e .env.production -e ../solana/.env.production -e ../core/.env.production -e .env -e ../solana/.env -e ../core/.env --",
    "prod:package": "serverless package --stage production",
    "prod:sls": "npm run prod:env -- serverless invoke local --stage production --function",
    "start": "node dist/cli.js",
    "start:ts": "node -r ts-node/register -r tsconfig-paths/register --inspect src/cli.ts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "@npc/core": "0.0.1",
    "@npc/solana": "0.0.1",
    "@orca-so/common-sdk": "^0.6.0",
    "@orca-so/whirlpools-sdk": "^0.13.0"
  },
  "author": "Mark Nemmer",
  "private": true,
  "license": "UNLICENSED",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0",
    "yarn": "Use npm!"
  }
}
