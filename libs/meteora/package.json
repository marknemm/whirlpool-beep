{
  "name": "@npc/meteora",
  "version": "0.0.1",
  "description": "Meteora Beep-Boop-Bop ğŸ¤–",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc --build",
    "build:root": "npm run build --include-workspace-root",
    "build:watch": "tsc --build --watch",
    "build:fn": "npm run prod:package",
    "clean": "rimraf dist tsconfig.tsbuildinfo .serverless",
    "clean:all": "npm run clean && npm run clean:deps && npm run clean:logs",
    "clean:deps": "rimraf node_modules",
    "clean:logs": "rimraf logs",
    "clean:root": "npm run clean --include-workspace-root",
    "dev": "npm run build && npm run dev:env -- npm run start:ts --",
    "dev:db": "npm run dev:db --prefix ../core",
    "dev:deploy": "npm run build && serverless deploy --stage development",
    "dev:env": "npm run dev:db && dotenv -e .env.development.local -e ../solana/.env.development.local -e ../core/.env.development.local -e .env.development -e ../solana/.env.development -e ../core/.env.development -e .env.local -e ../solana/.env.local -e ../core/.env.local -e .env -e ../solana/.env -e ../core/.env --",
    "dev:package": "npm run build && serverless package --stage development",
    "dev:sls": "npm run dev:env -- serverless invoke local --stage development --function",
    "install:root": "npm install --include-workspace-root",
    "lint": "eslint .",
    "prod": "npm run build && npm run prod:env -- npm run start:ts --",
    "prod:deploy": "npm run build && serverless deploy --stage production",
    "prod:env": "dotenv -e .env.production.local -e ../solana/.env.production.local -e ../core/.env.production.local -e .env.production -e ../solana/.env.production -e ../core/.env.production -e .env.local -e ../solana/.env.local -e ../core/.env.local -e .env -e ../solana/.env -e ../core/.env --",
    "prod:package": "npm run build && serverless package --stage production",
    "prod:sls": "npm run prod:env -- serverless invoke local --stage production --function",
    "start": "node dist/cli.js",
    "start:ts": "node -r ts-node/register -r tsconfig-paths/register --inspect src/cli.ts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "@meteora-ag/dlmm": "^1.1.4",
    "@npc/core": "0.0.1",
    "@npc/solana": "0.0.1",
    "@solana/spl-token": "^0.4.8",
    "@solana/web3.js": "^1.95.3",
    "axios": "^1.7.3",
    "bn.js": "^5.2.1",
    "bs58": "^5.0.0",
    "colors": "^1.4.0",
    "decimal.js": "^10.4.3",
    "deepmerge": "^4.3.1",
    "envalid": "^8.0.0",
    "glob": "^11.0.0",
    "kysely": "^0.27.4",
    "yargs": "^17.7.2"
  },
  "author": "Mark Nemmer",
  "license": "UNLICENSED",
  "private": true,
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0",
    "yarn": "Use npm!"
  }
}
